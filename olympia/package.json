{
  "name": "ecms_p12_epg_aggregator",
  "version": "1.0.0",
  "description": "setup: npm install -g tsd; tsd install mocha should --save; install jshint plugin in vscode;",
  "config": {
    "logLevel": "debug",
    "version": "9",
    "ecms": {
      "delta": "true",
      "startdate": "2016-08-06",
      "enddate": "2016-08-22",
      "proto": "https",
      "host": "eventcms.zdf.de",     
      "host1": "sofa.zdf.de",
      "path": "/xml/olympia2016/epg/",
      "path1": "/fakecms/"
    },
    "p12": {
      "proto": "http",
      "range": 17
    },
    "useragent": "axios (nodejs) - Olympia/ECMS (HRNM - Webmaster)"
  },
  "scripts": {
    "production:run": "DB=${npm_config_couchdb} logLevel=error node ./app/main.js",
    "production:install": "npm install; cd sofaapp; npm install -g kanso; kanso install; kanso push --minify ${npm_config_couchdb}; cd ..",
    "debug": "DB=${npm_config_couchdb} logLevel=${npm_package_config_logLevel} node ./app/main.js",
    "inspect": "DB=${npm_config_couchdb} logLevel=${npm_package_config_logLevel} node --debug-brk=5858 ./app/main.js",
    "debug:watch": "watch 'DB=${npm_config_couchdb} logLevel=${npm_package_config_logLevel} node ./app/main.js' app/",
    "debug:s1": "DB=${npm_config_couchdb_sofa1} node ./app/main.js",
    "debug:s2": "DB=${npm_config_couchdb_sofa2} node ./app/main.js",
    "push": "cd sofaapp; kanso push ${npm_config_couchdb}; cd ..",
    "push:s2": "cd sofaapp; kanso push ${npm_config_couchdb_sofa2}; cd ..",
    "push:s1": "cd sofaapp; kanso push ${npm_config_couchdb_sofa1}; cd ..",
    "push:watch": "watch 'npm run push' sofaapp/",
    "push:s2:watch": "watch 'npm run push:s2' sofaapp/"
  },
  "main": "./app/main.js",
  "author": "ronny adams",
  "license": "GPL-3.0",
  "readme": "readme.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/werty1st/epgservice.git#apiv2"
  },
  "dependencies": {
    "deep-diff": "^0.3.4",
    "memdown": "^1.1.2",
    "moment": "^2.11.0",
    "pouchdb": "^5.3.2",
    "pouchdb-upsert": "^2.0.0",
    "request": "^2.69.0",
    "stream": "0.0.2",
    "winston": "^2.2.0",
    "winston-daily-rotate-file": "^1.0.1",
    "xml-flow": "^0.4.0"
  },
  "devDependencies": {
    "jshint": "^2.9.1-rc2",
    "kanso": "^0.5.0",
    "watch": "^0.17.1"
  }
}
