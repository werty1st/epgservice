{
  "name": "ecms_p12_epg_aggregator",
  "version": "1.0.0",
  "description": "setup: npm install -g tsd; tsd install mocha should --save; install jshint plugin in vscode;",
  "config": {
    "logLevel": "debug",
    "version": "2",
    "ecms": {
      "delta": "false",
      "rscFix": "false",
      "startdate": "2017-12-11",
      "enddate": "2018-01-10",
      "startdate2": "2016-08-03",
      "enddate2": "2016-08-21",
      "proto": "https",
      "host": "int-ecms.zdf.de",
      "host2": "eventcms.zdf.de",
      "host1": "sofa.zdf.de",
      "path": "/xml/olympia2018/epg/",
      "path2": "/xml/olympia2016/epg/",
      "path1": "/fakecms/"
    },
    "api": {
      "startdate": "2017-12-11",
      "brands": [],
      "brands2": [
        {
          "brandname": "das aktuelle sportstudio",
          "brandid": "72e8070c-99c7-3573-bd1d-0c44ef6942b2"
        },
        {
          "brandname": "ZDF SPORTextra",
          "brandid": "145f6a3a-10b7-3a31-99dd-c3396369b148"
        },
        {
          "brandname": "heute nachrichten",
          "brandid": "c78223ab-14bd-3950-adeb-e5b0c7b0f049"
        }
      ],
      "delta": "false",
      "range": 2
    },
    "useragent": "axios (nodejs) - Olympia/ECMS (HRNM - Webmaster)"
  },
  "scripts": {
    "vscdebug": "logLevel=debug apiclient=${npm_config_apiclient} apisecret=${npm_config_apisecret} apihost=${npm_config_apihost} DB=${npm_config_DB} node --nolazy --inspect-brk=9229 ./app/main.js",
    "debug": "logLevel=debug apiclient=${npm_config_apiclient} apisecret=${npm_config_apisecret} apihost=${npm_config_apihost} DB=${npm_config_DB} node --nolazy ./app/main.js",
    "docker": "NODE_ENV=production node ./app/main.js",
    "docker:debug": "NODE_ENV=production logLevel=debug node ./app/main.js",

    "production:run": "DB=${npm_config_DB} logLevel=error node ./app/main.js",
    "production:install": "npm install; cd sofaapp; npm install -g kanso; kanso install; kanso push --minify ${npm_config_DB}; cd ..",
    "debug_old": "DB=${npm_config_DB} logLevel=${npm_package_config_logLevel} apiint=true apiclient=${npm_config_apiclient} apisecret=${npm_config_apisecret} node --nolazy --debug-brk=5858 ./app/main.js",
    "inspect": "DB=${npm_config_DB} logLevel=${npm_package_config_logLevel} node --debug-brk=5858 ./app/main.js",
    "debug:watch": "watch 'DB=${npm_config_DB} logLevel=${npm_package_config_logLevel} node ./app/main.js' app/",
    "debug:s1": "DB=${npm_config_couchdb_sofa1} node ./app/main.js",
    "push": "cd sofaapp; kanso push ${npm_config_DB}; cd ..",
    "push:s1": "cd sofaapp; kanso push ${npm_config_couchdb_sofa1}; cd ..",
    "push:watch": "watch 'npm run push' sofaapp/",
    "push:s2:watch": "watch 'npm run push:s2' sofaapp/",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "main": "./app/main.js",
  "author": "ronny adams",
  "license": "GPL-3.0",
  "readme": "readme.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/werty1st/epgservice.git#apiv2"
  },
  "dependencies": {
    "async": "^2.5.0",
    "deep-diff": "^0.3.8",
    "jsonpath": "^1.0.0",
    "memdown": "^1.1.2",
    "moment": "^2.11.0",
    "moment-timezone": "^0.5.13",
    "pouchdb": "^5.3.2",
    "pouchdb-upsert": "^2.0.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "stream": "0.0.2",
    "winston": "^2.3.1",
    "xml-flow": "^0.4.0"
  },
  "devDependencies": {
    "jshint": "^2.9.1-rc2",
    "kanso": "^0.5.0",
    "watch": "^0.17.1"
  }
}
