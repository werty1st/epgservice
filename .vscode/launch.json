    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch via NPM",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "vscdebug"
            ],
            "port": 9229
        },
        {
            "name": "debug S3",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "debug:s3:vsc"
            ],           
            "port": 9229
        }        
    ]
}